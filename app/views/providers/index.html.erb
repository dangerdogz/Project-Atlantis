<div class="container">
  <div class="container__head">
    <%= link_to t('providers.new_provider'), new_provider_path %>
  </div>

  <table>
    <% if @providers.any? %>
        <%= render @providers %>
    <% else %>
    <tr>
      <%= t('providers.no_providers') %>
    </tr>
    <% end %>
  </table>
</div>